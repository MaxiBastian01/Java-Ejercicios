class Punto{
    private int x;
    private int y;


    public Punto(int x, int y){
        this.x = x;
        this.y = y;
    }

    @Override
    public String toString() {
        return "Coordenadas: " + "\n" +
        "X: " + x + "\n" + 
        "Y: " + y;
    }

    public void setX(int newX){
        this.x = newX;
    }

    public void setY(int newY){
        this.y = newY;
    }

    
}
